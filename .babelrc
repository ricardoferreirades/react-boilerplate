{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "pure": true,
        "transpileTemplateLiterals": true,
        "namespace": "boilerplate",
        "meaninglessFileNames": ["index", "styles"]
      }
    ]
  ]
}
